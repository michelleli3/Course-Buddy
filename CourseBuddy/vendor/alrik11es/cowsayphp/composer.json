{
    "name": "league/cowsayphp",
    "description": "Cowsay port in PHP",
    "keywords": [
        "league",
        "cowsay"
    ],
    "homepage": "https://github.com/alrik11es/cowsayphp",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Sigueros",
            "email": "alrik11es@gmail.com",
            "homepage": "https://github.com/alrik11es",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Cowsayphp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Cowsayphp\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "bin/cowsayphp"
    ],
    "config":{
        "bin-dir": "bin"
    }
}
